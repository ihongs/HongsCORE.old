<?xml version="1.0" encoding="UTF-8"?>

<db xmlns="http://hongs-core"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://hongs-core db.xsd">
    <source link="default"/>
    <origin link="default"/>
    <tables>
        <table name="a_hcim_entity"
               primaryKey="id">
            <assoc type="BLS_TO"
                   name="a_hcim_module"
                   foreignKey="moduel_id"/>
            <assoc type="HAS_MANY"
                   name="a_hcim_entity_cols"
                   foreignKey="entity_id">
                <assoc type="BLS_TO"
                       name="a_hcim_domain"
                       foreignKey="domain_id"/>
            </assoc>
            <assoc type="HAS_MANY"
                   name="a_hcim_entity_rels"
                   foreignKey="entity_id">
                <assoc type="BLS_TO"
                       name="a_hcim_entity"
                       foreignKey="relate_id"/>
            </assoc>
            <assoc type="HAS_MANY"
                   name="a_hcim_entity_uniq"
                   foreignKey="entity_id">
                <assoc type="HAS_MANY"
                       name="a_hcim_entity_uniq_cols"
                       foreignKey="uniq_id">
                    <assoc type="BLS_TO"
                           name="uniq_cols"
                           tableName="a_hcim_entity_cols"
                           foreignKey="cols_id"/>
                </assoc>
                <assoc type="HAS_MANY"
                       name="a_hcim_entity_uniq_rels"
                       foreignKey="uniq_id">
                    <assoc type="BLS_TO"
                           name="uniq_rels"
                           tableName="a_hcim_entity_rels"
                           foreignKey="rels_id"/>
                </assoc>
            </assoc>
        </table>
        <table name="a_hcim_module"
               primaryKey="id"/>
        <table name="a_hcim_domain"
               primaryKey="id"/>
        <table name="a_hcim_entity_cols"
               primaryKey="id"/>
        <table name="a_hcim_entity_rels"
               primaryKey="id"/>
        <table name="a_hcim_entity_uniq"
               primaryKey="id"/>
        <table name="a_hcim_entity_uniq_cols"/>
        <table name="a_hcim_entity_uniq_rels"/>
    </tables>
</db>
