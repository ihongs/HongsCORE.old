<?xml version="1.0" encoding="UTF-8"?>

<db xmlns="http://hongs-core"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://hongs-core db.xsd">
    <config link="default"/>
    <driver link="default"/>
    <source link="default"/>
    <tables>
        <table name="a_hcrm_datasrc"
               primaryKey="id">
            <assoc type="HAS_MANY"
                   name="a_hcrm_dataset_conf"
                   foreignKey="datasrc_id"/>
        </table>
        <table name="a_hcrm_dataset"
               primaryKey="id">
            <assoc type="BLS_TO"
                   name="a_hcrm_datasrc"
                   foreignKey="datasrc_id"/>
            <assoc type="HAS_MANY"
                   name="a_hcrm_dataset_conf"
                   foreignKey="dataset_id"/>
            <assoc type="HAS_MANY"
                   name="a_hcrm_dataset_cols"
                   foreignKey="dataset_id"
                   updateKeys="name,type,value_type,value_size,value_scale"
                   orderBy=".type,.name"/>
        </table>
        <table name="a_hcrm_loadlog"
               primaryKey="id">
            <assoc type="BLS_TO"
                   name="a_hcrm_dataset"
                   foreignKey="dataset_id"/>
            <assoc type="HAS_MANY"
                   name="a_hcrm_loadlog_dims"
                   foreignKey="loadlog_id"/>
        </table>
        <table name="a_hcrm_datasrc_conf"
               primaryKey="id"/>
        <table name="a_hcrm_dataset_conf"
               primaryKey="id"/>
        <table name="a_hcrm_dataset_cols"
               primaryKey="id"/>
        <table name="a_hcrm_loadlog_dims"
               primaryKey="id"/>
    </tables>
</db>