<?xml version="1.0" encoding="UTF-8"?>

<db xmlns="http://hongs-core"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://hongs-core db.xsd">
    <source link="default"/>
    <driver link="default"/>
    <tables>
        <table name="a_hcum_user"
               primaryKey="id">
            <assoc type="HAS_MANY"
                   name="a_hcum_user_group"
                   foreignKey="user_id"/>
            <assoc type="HAS_MANY"
                   name="a_hcum_user_dept"
                   foreignKey="user_id">
                <assoc type="BLS_TO"
                       name="a_hcum_dept"
                       foreignKey="dept_id"/>
            </assoc>
        </table>
        <table name="a_hcum_dept"
               primaryKey="id">
            <assoc type="HAS_MANY"
                   name="a_hcum_dept_group"
                   foreignKey="dept_id"/>
        </table>
        <table name="a_hcum_user_dept"/>
        <table name="a_hcum_user_group"/>
        <table name="a_hcum_dept_group"/>
    </tables>
</db>
