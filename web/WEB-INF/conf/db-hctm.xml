<?xml version="1.0" encoding="UTF-8"?>

<db xmlns="http://hongs-core"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://hongs-core db.xsd">
    <config link="default"/>
    <driver link="default"/>
    <source link="default"/>
    <tables>
        <table name="a_hctm_container"
               primaryKey="id">
        </table>
        <table name="a_hctm_code"
               primaryKey="id">
            <assoc type="BLS_TO"
                   name="a_hctm_container"
                   foreignKey="container_id"/>
            <assoc type="HAS_MANY"
                   name="a_hctm_code_rule"
                   foreignKey="code_id">
                <assoc type="BLS_TO"
                       name="a_hctm_rule"
                       foreignKey="rule_id"/>
            </assoc>
        </table>
        <table name="a_hctm_rule"
               primaryKey="id">
            <assoc type="BLS_TO"
                   name="a_hctm_container"
                   foreignKey="container_id"/>
            <assoc type="HAS_MANY"
                   name="a_hctm_rule_item"
                   foreignKey="rule_id">
                <assoc type="BLS_TO"
                       name="a_hctm_var"
                       foreignKey="var_id"/>
            </assoc>
        </table>
        <table name="a_hctm_var"
               primaryKey="id">
            <assoc type="BLS_TO"
                   name="a_hctm_container"
                   foreignKey="container_id"/>
        </table>
        <table name="b_hctm_container"></table>
    </tables>
</db>