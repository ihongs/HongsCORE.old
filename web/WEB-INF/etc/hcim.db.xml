<?xml version="1.0" encoding="utf-8" standalone="no"?>
<db xmlns="http://hongs-core" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hongs-core .db.xsd">
    <source link="hcim"/>
    <origin link="hcim"/>
    <tables>
        <table name="user"
               primaryKey="id"
               model="app.hongs.db.serv.HcumUser">
            <assoc type="HAS_MANY"
                   name="user_group"
                   foreignKey="user_id"/>
            <assoc type="HAS_MANY"
                   name="user_dept"
                   foreignKey="user_id">
                <assoc type="BLS_TO"
                       name="dept"
                       foreignKey="dept_id"/>
            </assoc>
        </table>
        <table name="dept"
               primaryKey="id"
               model="app.hongs.db.serv.HcumDept">
            <assoc type="HAS_MANY"
                   name="dept_group"
                   foreignKey="dept_id"/>
        </table>
        <table name="user_dept"/>
        <table name="user_group"/>
        <table name="dept_group"/>
    </tables>
</db>