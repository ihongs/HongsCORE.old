<?xml version="1.0" encoding="UTF-8"?>
<root>
    <unit name="__MENU__">
    </unit>
    <unit name="_search">
        <item disp="ID" name="id" type="hidden"/>
        <item disp="检索" name="wd" type="hidden">
            <attrib name="field_class">text</attrib>
            <attrib name="field_store">no</attrib>
        </item>
        <item disp="名称" name="name" type="text"/>
        <item disp="说明" name="note" type="textarea">
            <attrib name="field_class">stored</attrib>
        </item>
        <item disp="数据" name="data" type="textarea" rule="pass">
            <attrib name="field_class">json</attrib>
        </item>
        <item disp="标识" name="identity" type="number">
            <attrib name="type">int</attrib>
        </item>
        <item disp="省份" name="province" type="number">
            <attrib name="type">int</attrib>
        </item>
        <item disp="行业" name="trade_id" type="number" repeated="2">
            <attrib name="type">int</attrib>
        </item>
    </unit>
</root>
